<h1>Registros</h1> 
<div class="container">
        <div class="table-responsive">
        <table class="table table-dark table-striped">
        <tr>
                <th scope="col">Nome</th>
                <th scope="col">Email</th>
                <th scope="col">Telefone</th>
                <th scope="col">Ações<th>
        </tr>
        <tr>
            {{#each posts}}
                <td>{{nome}}</td>
                <td>{{email}}</td>
                <td>{{telefone}}</td>
                <td>{{problema}}</td>
                <td><a href="/deletar/{{id}}"><button>Deletar</button></a></td>
        </tr>
            {{/each}}

        </table>

        </div>
</div>



